.navtabBox {
  position: fixed;
  bottom:0;
  left:0;
  right: 0;
  display: flex;
  flex: row;
  align-items: center;
  justify-content: center;
  z-index: 10;
  background: #fff;
  border-top: 1rpx solid #f2f2f2;
  animation: navBottomBarshow .6s ease;
}

.navTabItem {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100rpx;
}

.navTabIcon {
  width: 42rpx;
  height: 42rpx;
}

.navTabText {
  margin-top: 8rpx;
  font-size: 20rpx;
  color: #323232;
}

.navselectColor {
  color: #FF2332;
  animation: scalin .4s;
}


@keyframes scalin {
  0% {
      transform: scale(1);
     
  }

  50% {
      transform: scale(1.2)
  }

  100% {
      transform: scale(1);
  }
}

@keyframes navBottomBarshow {
  0% {
      opacity: 0.8;
      transform: translate3d(0,160%, 0);
  }

  100% {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}