.page-content {
    position: fixed;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    z-index: -1;
}
.null-box{
    position: relative;
}
.null-text{
    position: absolute;
    font-size: 28rpx;
    color:#999;
}
.first{
    right: -42rpx;
    top: -21rpx;
    animation: scale 2s linear;
}
.second{
    right: -82rpx;
    top: -50rpx;
    animation: scale 3s linear;
}
.nullIcon {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    filter: grayscale(100%);
    filter: gray;
    opacity: 0.7;

}

@keyframes scale {
    0% {
        transform: scale(.8);
        opacity: 0
    }
  
    50% {
        transform: scale(1.3)
    }
  
    100% {
        transform: scale(1);
        opacity: 1
    }
  }